# Example environment file for Bash Dot Env Library
# This file demonstrates the supported .env format

# Application settings
APP_NAME=TestApp
API_KEY="secret-key-123"
PORT=3000
DEBUG=true # Inline comment example

# Database configuration  
DB_HOST=localhost
DB_PORT=5432

# Values with spaces (quotes preserved)
QUOTED_VALUE="value with spaces"

# Special characters
SPECIAL_CHARS=value@#$%

# ARM/Azure example variables
ARM_SUBSCRIPTION_ID="123123-12312-123123-123"
ARM_AAAAID="2222-2222-2222-2222" # Comment
ARM_TENANT_ID=5555-5555-555-555
COUNT=2
FOO=true